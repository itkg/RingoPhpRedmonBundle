{% extends 'RingoPhpRedmonBundle::base.html.twig' %}

{% block main %}
    <h2>Configuration</h2>
    <table class="table table-striped">
        <thead>
            <th>Name </th>
            <th>Value </th>                    
        </thead>
    {% for key,value in configs %}
    <tr>
        <td>{{key}}</td>
        <td>{{value}}</td>

    </tr>
    {% endfor %}
    </table>
      
{% endblock %}            
